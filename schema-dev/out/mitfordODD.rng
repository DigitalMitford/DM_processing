         </zeroOrMore>
         <ref name="teiatt.global.attributes"/>
         <ref name="teiatt.datable.attributes"/>
         <ref name="teiatt.dimensions.attributes"/>
         <ref name="teiatt.editLike.attributes"/>
         <ref name="teiatt.typed.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="teiorigPlace">
      <element name="origPlace">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(origin place) contains any form of place name, used to identify the place of origin for a manuscript or manuscript part. [10.3.1. Origination]</a:documentation>
         <ref name="teimacro.phraseSeq"/>
         <ref name="teiatt.global.attributes"/>
         <ref name="teiatt.naming.attributes"/>
         <ref name="teiatt.datable.attributes"/>
