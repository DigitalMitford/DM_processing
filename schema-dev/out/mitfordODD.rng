         <ref name="teiatt.typed.attributes"/>
         <ref name="teiatt.global.attributes"/>
         <ref name="teiatt.media.attribute.width"/>
         <ref name="teiatt.media.attribute.height"/>
         <ref name="teiatt.media.attribute.scale"/>
         <ref name="teiatt.resourced.attributes"/>
         <ref name="teiatt.declaring.attributes"/>
         <ref name="teiatt.timed.attributes"/>
         <attribute name="mimeType">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(MIME media type) specifies the applicable multimedia internet mail extension (MIME) media type</a:documentation>
            <list>
               <oneOrMore>
                  <data type="token">
                     <param name="pattern">(\p{L}|\p{N}|\p{P}|\p{S})+</param>
                  </data>
               </oneOrMore>
            </list>
         </attribute>
         <empty/>
      </element>
