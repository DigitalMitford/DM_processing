   <define name="teiatt.coordinated.attributes">
      <ref name="teiatt.coordinated.attribute.start"/>
      <ref name="teiatt.coordinated.attribute.ulx"/>
      <ref name="teiatt.coordinated.attribute.uly"/>
      <ref name="teiatt.coordinated.attribute.lrx"/>
      <ref name="teiatt.coordinated.attribute.lry"/>
      <ref name="teiatt.coordinated.attribute.points"/>
