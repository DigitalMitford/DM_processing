            <optional>
               <ref name="teibloc"/>
            </optional>
            <optional>
               <ref name="teicountry"/>
            </optional>
            <optional>
               <ref name="teiregion"/>
            </optional>
            <optional>
               <ref name="teisettlement"/>
            </optional>
            <optional>
               <ref name="teidistrict"/>
            </optional>
            <optional>
               <ref name="teigeogName"/>
            </optional>
     
            <optional>
               <ref name="teiinstitution"/>
            </optional>
     
     
            <optional>
               <ref name="teirepository"/>
            </optional>
     
     
            <optional>
               <ref name="teicollection"/>
            </optional>
     
            <ref name="teiidno"/>
     
            <optional>
               <ref name="teinote"/>
            </optional>
     
         </group>
