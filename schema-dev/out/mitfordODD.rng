            <optional>
               <ref name="teifront"/>
            </optional>
      
      
            <oneOrMore>
               <choice>
                  <ref name="teimodel.graphicLike"/>
                  <ref name="teisurface"/>
                  <ref name="teisurfaceGrp"/>
